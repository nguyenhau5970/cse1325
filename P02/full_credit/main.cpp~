#include <iostream>
#include "pet.h"

int main() 
{
    Pet P(Pluto,10,DOG);
    Pet F(Felicia,7,CAT);
    Pet E(Eliza,12,PEN);
    Pet M(Marth,23,ELE);
    std::cout << P.to_string() << std::endl;
    std::cout << F.to_string() << std::endl;
    std::cout << E.to_string() << std::endl;
    std::cout << M.to_string() << std::endl;
}
